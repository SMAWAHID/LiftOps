apiVersion: v1
kind: Service
metadata:
  name: liftops-service
spec:
  selector:
    app: liftops
  ports:
    - protocol: TCP
      port: 80  # Expose port 80 externally
      targetPort: 8005  # The internal port your app is running on
  type: LoadBalancer  # Use LoadBalancer to expose to the public internet

